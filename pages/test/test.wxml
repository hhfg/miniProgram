<!--index//pkPage/pkPage.wxml-->
<view class="rival">
  <view class="player1">
    <open-data type="userAvatarUrl"  class="img1"></open-data>
  </view>
  <canvas class="progress_bg"   canvas-id="canvasProgressbg">  </canvas> 
  <canvas class="progress_canvas" canvas-id="secondCanvas"></canvas>
  <view class="player2">
    <open-data type="userAvatarUrl"  class="img1"></open-data>
  </view>
</view>

<view class="chooseSection">
  <progress class="ascore" percent="50" stroke-width="14" style="width:{{height}}px;margin-top:{{top}}px;margin-right:{{right}}px"></progress>
  <progress class="bscore" percent="50" stroke-width="14" style="width:{{height}}px;margin-left:{{left}}px;margin-top:{{top}}px"></progress>
</view>




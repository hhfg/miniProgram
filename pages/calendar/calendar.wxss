.calendar{
  width: 100%;
  text-align: center;
  font-size: 30rpx;
  box-sizing: border-box;
}
.canlendar-title{
  line-height: 70rpx;
  font-size: 30rpx;
  text-align: left;
  padding: 0 20rpx;
  box-sizing: border-box;
}
.canlendar-title .ctrl{
  display: inline-block;
  padding: 0 20rpx;
  background: #f5f5f5;
  border-radius: 10rpx;
}
.calendar-title .item{
  display: inline-block;
  vertical-align: middle;
  line-height: 50rpx;
}
.canlendar-title .title{
  min-width: 300rpx;
  text-align: center;
}